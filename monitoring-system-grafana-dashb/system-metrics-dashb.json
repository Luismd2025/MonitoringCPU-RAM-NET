      "title": "Memory Usage real TIME (%)",
      "id": 2,
      "datasource": "Prometheus",
      "targets": [
        {
          "expr": "memory_usage_percent",
          "refId": "B"
        }
      ],
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "max": 100
        }
      }
